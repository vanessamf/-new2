<script>
    (function($) {
//        Select2 选择框配置
        $('#sel_category').select2({
            placeholder: "全部"
        });
        // 点击复制菜谱中需要配置的 select选择框
        $('#copy_sel_brand').select2({});

        //        分页设置
        var options = {
            alignment: "right",  //靠右
            bootstrapMajorVersion: 3, //对应的bootstrap版本
            currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
            numberOfPages: 5, //每页页数
            totalPages: 4, //总页数，EL表达式${totalPages }，获取从后台传过来的值
            shouldShowPage: true,  //是否显示该按钮
            /*
             //点击事件
             onPageClicked: function (event, originalEvent, type, page) {
             location.href = "url/&page=" + page;
             }*/
        };
        $('#ft_pagination').bootstrapPaginator(options);

    })(jQuery)
</script>

<script type="text/javascript">
    // 自适应 设置高度100%
    function reLay(){
        var pbox = $('#menu_content .pbox'),
                prevAll_list = pbox.prevAll(),
                top_H = 0

        for(var i=0;i<prevAll_list.length;i++){
            top_H += prevAll_list[i].offsetHeight;
        }

        pbox.css({top:top_H});
    }
    window.onresize = reLay;
    (function($){
        reLay();

        var $brand_list = $("#brand_list");
        // 下划线跟随导航滑动效果
        $brand_list.lavaLamp({
            fx: "backout",
            speed: 700,
            click: function(event, menuItem) {
                return false;
            }
        });
    })(jQuery);
</script>
