<!-- 创建编辑员工信息-->
<div class="modal" id="cre_mod_staff" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content wd-sm">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">创建员工账号</h4>
            </div>
            <div class="modal-body">
                <form class="staff-info-form" action role="form">
                    <div class="form-group">
                        <label class="col-sm-5 control-label">权限：</label>
                        <div class="col-sm-19">
                            <div class="btn-group m-r select-base">
                                <button data-toggle="dropdown" class="select-btn  dropdown-toggle">
                                    <span class="dropdown-label">员工组</span>
                                    <span class="sel-arrow"><b></b></span>
                                </button>
                                <ul class="dropdown-menu dropdown-select">
                                    <li class="active"><a href="#"><input type="radio" name="d-s-r"> 员工组</a></li>
                                    <li><a href="#"><input type="radio" name="d-s-r"> 管理组</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-5 control-label">员工姓名：</label>
                        <div class="col-sm-19">
                            <input type="text" name="delivery-km" class="form-ipt input-sm">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-5 control-label">员工编号：</label>
                        <div class="col-sm-19">
                            <input type="text" name="delivery-km" class="form-ipt input-sm">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-5 control-label">密码：</label>
                        <div class="col-sm-19">
                            <input type="password" name="password" class="form-ipt input-sm">
                        </div>
                    </div>
                   <!-- <div class="form-group">
                        <label class="col-sm-5 control-label">状态：</label>
                        &lt;!&ndash;<div class="col-sm-19">
                           <div class="slideThree">
                               <input type="checkbox" value="None" id="slideThree" name="check" checked="">
                               <label for="slideThree"></label>
                           </div>
                       </div>&ndash;&gt;
                        <div class="col-sm-19">
                            <ul class="ul-list">
                                <li class="choice-item"><input name="sel-time" id="rad_status_enabled" type="radio" class="ipt-choice ipt-rad input-sm" checked=""><label class="choice-lab rad-lb" for="rad_status_enabled"></label><span>启用</span></li>
                                <li class="choice-item"><input name="sel-time" id="rad_status_disabled" type="radio" class="ipt-choice ipt-rad input-sm"><label class="choice-lab rad-lb" for="rad_status_disabled"></label><span>停用</span></li>
                            </ul>
                        </div>
                    </div>-->
                    <div class="form-group">
                        <label class="col-sm-5 control-label">状态：</label>
                        <div class="col-sm-19 ds-line">
                            <div class="switch-state-control">
                                <!-- 获取选中状态 $('.toggle-state')[0].checked  true: 代表启用; false : 表示停用-->
                                <input type="checkbox" id="toggle_switch_state" class="toggle-state" checked="checked">
                                <label for="toggle_switch_state"></label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group ops-group">
                        <ul class="ul-list modal-ops">
                            <li><a class="modal-btn bg-light-blue" data-dismiss="modal" aria-hidden="true">取消</a></li>
                            <li><a class="modal-btn bg-m-blue">确定</a></li>
                        </ul>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
