	<script type="text/javascript">
		// 自适应 设置高度100%
		function reLay() {
			var pbox = $('#menu_content .pbox'),
				prevAll_list = pbox.prevAll(),
				top_H = 0
			for(var i = 0; i < prevAll_list.length; i++) {
				top_H += prevAll_list[i].offsetHeight;
			}

			pbox.css({
				top: top_H
			});
		}
		window.onresize = reLay;
		(function($) {
			reLay();

			var $brand_list = $("#brand_list");
			// 下划线跟随导航滑动效果
			$brand_list.lavaLamp({
				fx: "backout",
				speed: 700,
				click: function(event, menuItem) {
					return false;
				}
			});
		})(jQuery);
		(function($) {
			// 点击复制菜谱中需要配置的 select选择框
			//			$('#sea_staff_menu').select2({});

			//        分页设置
			var options = {
				alignment: "", //靠右
				bootstrapMajorVersion: 3, //对应的bootstrap版本
				currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
				numberOfPages: 5, //每页页数
				totalPages: 10, //总页数，EL表达式${totalPages }，获取从后台传过来的值
				shouldShowPage: true, //是否显示该按钮
				itemTexts: function(type, page, current) {
					switch(type) {
						case "first":
							return "第一页";
						case "prev":
							return "<";
						case "next":
							return ">";
						case "last":
							return "末页";
						case "page":
							return page;
					}
				},
				itemContainerClass: function(type, page, current) {
					//					debugger;
					if(page === current) {
						if(page === 1 || page === this.totalPages) {
							return "active disabled";
						} else {
							return "active"
						}
					} else return ""
				},
				onPageClicked: function(type, page) {
					//					alert(page.data.page);
					if(page.data.page === 1) {
						$("ul.pagination li:nth-child(3) a").css({
							"color": "#ffffff",
							"background-color": "#1a5cc6",
							"border-color": "#1a5cc6"
						})
					}
				}
			};
			$('#ft_pagination_menu').bootstrapPaginator(options);
		})(jQuery);

