	<script type="text/javascript">
		subquanxuan("allChecked", "desks-item");
		(function($) {
			//        分页设置
			var options = {
				alignment: "", //靠右
				bootstrapMajorVersion: 3, //对应的bootstrap版本
				currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
				numberOfPages: 5, //每页页数
				totalPages: 10, //总页数，EL表达式${totalPages }，获取从后台传过来的值
				shouldShowPage: true, //是否显示该按钮
				itemTexts: function(type, page, current) {
					switch(type) {
						case "first":
							return "第一页";
						case "prev":
							return "<";
						case "next":
							return ">";
						case "last":
							return "末页";
						case "page":
							return page;
					}
				},
				itemContainerClass: function(type, page, current) {
					if(page === current) {
						if(page === 1 || page === this.totalPages) {
							return "active disabled";
						} else {
							return "active"
						}
					} else return ""
				}
			};
			$('#ft_pagination_menu').bootstrapPaginator(options);
		})(jQuery);
	</script>
	<script>
		(function($) {
			$(".start-date").datetimepicker({
				language: "zh-CN",
				format: 'yyyy-mm-dd',
				//				todayBtn : 1,
				autoclose: true,
				startView: 2,
				minView: 2,
				pickerPosition: 'bottom-left',
				minuteStep: 5,
				//				startDate:"2018-11-11"
				//				bootcssVer:3
			}).on('changeDate', function(event) {
				event.preventDefault();
				event.stopPropagation();
				var startTime = event.date;
				$(".end-date").datetimepicker('setStartDate', startTime);
				$(".end-date").val("");
			});

			$(".end-date").datetimepicker({
				language: "zh-CN",
				format: 'yyyy-mm-dd',
				autoclose: true,
				startView: 2,
				minView: 2,
				pickerPosition: 'bottom-left',
				//				bootcssVer:2
			}).on('changeDate', function(event) {
				event.preventDefault();
				event.stopPropagation();
				var endTime = event.date;
				$('.start-date').datetimepicker('setEndDate', endTime);
			});
		})(jQuery);
	</script>
	<script>
		(function($) {
			//类型数据
			var initdata = ["请选择", "直营", "加盟"];
			$('#sel_category').select2({
				data: initdata,
				escapeMarkup: function(markup) {
					return markup;
				},
				//  minimumInputLength: 1,
				formatResult: function(row) { //选中后select2显示的 内容
					return row.name;
				},
				formatSelection: function(row) { //选择的时候，需要保存选中的id
					$("#personid").val(row.id);
					return row.name; //选择时需要显示的列表内容
				}
			});
		})(jQuery);
	</script>

