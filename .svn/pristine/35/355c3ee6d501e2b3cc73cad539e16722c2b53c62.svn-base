	<script>
		subquanxuan("allChecked", "type-list");
		(function($) {
			$(".form-horizontal").Validform({
				tiptype: function(msg, o, cssctl) {
					var obj_tip;
					if(!o.obj.is("form")) { //验证表单元素时o.obj为该表单元素，全部验证通过提交表单时o.obj为该表单对象;
						obj_tip = o.obj.siblings(".Validform_checktip");
						cssctl(obj_tip, o.type);
						obj_tip.text(msg);
					} else {
						obj_tip = o.obj.find("#msgdemo");
						cssctl(obj_tip, o.type);
						obj_tip.text(msg);
					}
				},
				ajaxPost: true,
				callback: function(form) {
					form[0].submit();
					return false;
				}
			});
		})(jQuery);
		(function($) {
			$('#deleteTables').on('shown.bs.modal', function() {
				scrollWidth1 = $(".type-list").width() - $(".type-list li").outerWidth();
				scrollWidth2 = $(".dish-list").width() - $(".dish-list li").outerWidth();
				paddingRight1 = (15 + scrollWidth1) + "px";
				paddingRight2 = (15 + scrollWidth2) + "px";
				$(".type-title").eq(0).css("paddingRight", paddingRight1);
				$(".type-title").eq(1).css("paddingRight", paddingRight2);
			})
		})(jQuery);
	</script>

	<script type="text/javascript">
		(function($) {
			//        分页设置
			var options = {
				alignment: "", //靠右
				bootstrapMajorVersion: 3, //对应的bootstrap版本
				currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
				numberOfPages: 5, //每页页数
				totalPages: 10, //总页数，EL表达式${totalPages }，获取从后台传过来的值
				shouldShowPage: true, //是否显示该按钮
				itemTexts: function(type, page, current) {
					switch(type) {
						case "first":
							return "第一页";
						case "prev":
							return "<";
						case "next":
							return ">";
						case "last":
							return "末页";
						case "page":
							return page;
					}
				},
				itemContainerClass: function(type, page, current) {
					//					debugger;
					if(page === current) {
						if(page === 1 || page === this.totalPages) {
							return "active disabled";
						} else {
							return "active"
						}
					} else return ""
				}
			};
			$('#ft_pagination_menu').bootstrapPaginator(options);
		})(jQuery);
	</script>

