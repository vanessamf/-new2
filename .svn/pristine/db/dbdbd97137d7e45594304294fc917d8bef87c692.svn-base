	<script>
		(function($) {
			//校验
			// 显示位置配置
			var config = {
				position: {
					at: 'center right' // tip显示位置
				}
			}
			// 初始化验证对象
			$(".ivalidate-box").each(function() {
				iValidate.init(this, config);
			});

		})(jQuery);
	</script>
	<script type="text/javascript">
		(function($) {
			//        分页设置
			var options = {
				alignment: "", //靠右
				bootstrapMajorVersion: 3, //对应的bootstrap版本
				currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
				numberOfPages: 5, //每页页数
				totalPages: 10, //总页数，EL表达式${totalPages }，获取从后台传过来的值
				shouldShowPage: true, //是否显示该按钮
				itemTexts: function(type, page, current) {
					switch(type) {
						case "first":
							return "第一页";
						case "prev":
							return "<";
						case "next":
							return ">";
						case "last":
							return "末页";
						case "page":
							return page;
					}
				},
				itemContainerClass: function(type, page, current) {
					if(page === current) {
						if(page === 1 || page === this.totalPages) {
							return "active disabled";
						} else {
							return "active"
						}
					} else return ""
				}
			};
			$('#ft_pagination_menu').bootstrapPaginator(options);
		})(jQuery);
	</script>

