	<script>
		(function($) {
			subquanxuan("allChecked", "type-list");
			//显示打印机
			$(document).on("click",".print-select li",function(){
				if($(".network-printer").hasClass("active")){
					$(".ipAddress").show();
					$(".drive").hide();
				}else{
					$(".ipAddress").hide();
					$(".drive").show();
				}
			})
		})(jQuery);
		
		(function($) {
			scrollWidth1 = $(".type-list").width() - $(".type-list li").outerWidth();
			scrollWidth2 = $(".dish-list").width() - $(".dish-list li").outerWidth();
			paddingRight1 = (15 + scrollWidth1) + "px";
			paddingRight2 = (15 + scrollWidth2) + "px";
			$(".type-title").eq(0).css("paddingRight", paddingRight1);
			$(".type-title").eq(1).css("paddingRight", paddingRight2);
		})(jQuery);
	</script>
	<script>
		(function($) {
			$(".validity-date").datetimepicker({
