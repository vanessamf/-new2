<script>
	$(document).ready(function() {
		//查询地址
		var query = window.location.href.substring(1);
		//分割地址
		var pairs = query.split("/");
		//查询最后一项
		var args = pairs[pairs.length - 1];
		var dataTxt = args.split(".")[0];
		
//		var curA = $("[data-name=" + dataTxt + "]");
		var curA;
		var curAList=$(".left-menu li a");
		for(var i=0;i<curAList.length;i++){
			var curAArray=curAList.eq(i).attr("data-name")?curAList.eq(i).attr("data-name").split(","):"";
			if($.inArray(dataTxt, curAArray)>=0){
				curA=curAList.eq(i);
			}
		}
		$(".subList-nav").hide();
		$(".left-menu>li").find(".sup-arrow").addClass("icon-you3");
		$(".left-menu>li").find(".sup-arrow").removeClass("icon-xia1");
		$(".left-menu>li").removeClass("active");
		$(".subList-nav li a").removeClass("active");
		curA.addClass("active");
		curA.parents(".subList-nav").show();
		curA.parents(".left-item").addClass("active");
		curA.parents(".left-item").find(".sup-arrow").removeClass("icon-you3");
		curA.parents(".left-item").find(".sup-arrow").addClass("icon-xia1");
//	 	$("#nav").css("visibility","visible");
	})
</script>