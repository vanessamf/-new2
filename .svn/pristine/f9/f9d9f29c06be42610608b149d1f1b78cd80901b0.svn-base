	<script type="text/javascript">
		$(document).ready(function() {
			$(".m-nav-primary .left-menu .left-item").find("a").first().trigger("click");
			$(".m-nav-primary .left-menu .left-item").find(".subList-nav a").first().trigger("click");
		});
		//侧边菜单
		$(document).on('click', '.left-menu>li>a', function(e) {
			var $this = $(e.target);
			//点击父菜单的时候的效果
			if($(this).parent().find("ul").is(':visible')) {
				$(this).find(".sup-arrow").addClass("icon-you-copy");
				$(this).find(".sup-arrow").removeClass("icon-xia1");
				$(this).next().slideUp(200);
			} else {
				$(this).parent().siblings("li").find("ul").slideUp(200);
				$(this).parent().siblings("li").find(".sup-arrow").addClass("icon-you-copy");
				$(this).parent().siblings("li").find(".sup-arrow").removeClass("icon-xia1");
				$(this).find(".sup-arrow").removeClass("icon-you-copy");
				$(this).find(".sup-arrow").addClass("icon-xia1");
				$(this).next().slideDown(200);
			}
		});
		$(document).on('click', '.left-menu li a', function(e) {
			if($(this).parent().children("ul").length == 0) {
				$(".left-menu>li").removeClass("active");
				$(".subList-nav li a").removeClass("active");
				$(this).parents(".left-item").addClass("active");
				$(this).addClass("active");
			}
			$("#menu_content").load($(this).attr("data-url"), function() {
				paginator();
				tableWrapperHeight();
				autoWidth();
				dropdownSelect();
			});
			$(document).attr("title", $(this).attr("data-title"));
		});

		var paginator = function() {
			//        分页设置
			var options = {
				alignment: "", //靠右
				bootstrapMajorVersion: 3, //对应的bootstrap版本
				currentPage: 1, //当前页数，EL表达式${currentPage }，获取从后台传过来的值
				numberOfPages: 5, //每页页数
				totalPages: 10, //总页数，EL表达式${totalPages }，获取从后台传过来的值
				shouldShowPage: true, //是否显示该按钮
				itemTexts: function(type, page, current) {
					switch(type) {
						case "first":
							return "第一页";
						case "prev":
							return "<";
						case "next":
							return ">";
						case "last":
							return "末页";
						case "page":
							return page;
					}
				},
				itemContainerClass: function(type, page, current) {
					if(page === current) {
						if(page === 1 || page === this.totalPages) {
							return "active disabled";
						} else {
							return "active"
						}
					} else return ""
				},
				//				onPageClicked: function(type, page) {
				//					if(page.data.page === 1) {
				//						$("ul.pagination li:nth-child(3) a").css({
				//							"color": "#ffffff",
				//							"background-color": "#1a5cc6",
				//							"border-color": "#1a5cc6"
				//						})
				//					}
				//				}
			};
			$('#ft_pagination_menu').bootstrapPaginator(options);
		}
	</script>

