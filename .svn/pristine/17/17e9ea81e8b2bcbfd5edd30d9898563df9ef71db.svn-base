<div class="modal" id="modal_delivery_st" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">配送设置</h4>
            </div>
            <div class="modal-body">
                <form action role="form">
                    <div class="form-group">
                        <label class="col-xs-5 col-sm-5 col-md-5 control-label">配送范围：</label>
                        <div class="col-xs-19 col-sm-19 col-md-19">
                            <input type="text" name="delivery-km" class="form-ipt input-sm"> 公里（方圆）
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-xs-5 col-sm-5 col-md-5 control-label">起送费：</label>
                        <div class="col-xs-19 col-sm-19 col-md-19">
                            <input type="text" name="delivery-km" class="form-ipt input-sm"> 元
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-xs-5 col-sm-5 col-md-5 control-label">配送费：</label>
                        <div class="col-xs-19 col-sm-19 col-md-19">
                            <input type="text" name="delivery-km" class="form-ipt input-sm"> 元
                        </div>
                    </div>
                    <div class="form-group ops-group">
                        <ul class="ul-list modal-ops">
                            <li><a class="modal-btn bg-light-blue" data-dismiss="modal" aria-hidden="true">取消</a></li>
                            <li><a class="modal-btn bg-m-blue">确定</a></li>
                        </ul>
                    </div>
                </form>
            </div>
        </div>
    </div>
