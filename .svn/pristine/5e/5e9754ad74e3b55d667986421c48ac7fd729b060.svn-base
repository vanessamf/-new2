	<script>
		(function($) {
			subquanxuan("allDishChecked", "list-container");
			$('[name="status"]').bootstrapSwitch({
				onText: "开",
				offText: "关",
				onColor: "info",
				offColor: "info",
				size: "large",
				state: false,
				//			animate:false,
				onSwitchChange: function(event, state) {
					if(state == true) {
						$(this).val("1");
					} else {
						$(this).val("2");
					}
				}
			});
		})(jQuery);
		(function($) {
			scrollWidth1 = $(".type-list").width() - $(".type-list li").outerWidth();
			scrollWidth2 = $(".dish-list").width() - $(".dish-list li").outerWidth();
			paddingRight1 = (15 + scrollWidth1) + "px";
			paddingRight2 = (15 + scrollWidth2) + "px";
			$(".type-title").eq(0).css("paddingRight", paddingRight2);
			$(".type-title").eq(1).css("paddingRight", paddingRight2);
		})(jQuery);
	</script>
	<script>
		(function($) {
			$(".validity-date").datetimepicker({
				language: "zh-CN",
				format: 'hh:ii:ss',
				//				todayBtn : 1,
				autoclose: true,
				startView: 0,
				minView: 0,
				maxView: 0,
				pickerPosition: 'top-left',
				minuteStep: 5,
				//				startDate:"2018-11-11"
				//				bootcssVer:3
			}).on('changeDate', function(event) {
				event.preventDefault();
				event.stopPropagation();
				var startTime = event.date;
				$(".deadline-date").datetimepicker('setStartDate', startTime);
				$(".deadline-date").val("");
			});

			$(".deadline-date").datetimepicker({
				language: "zh-CN",
				format: 'hh:ii:ss',
				autoclose: true,
				startView: 0,
				minView: 0,
				maxView: 0,
				pickerPosition: 'top-left',
				//				bootcssVer:2
			}).on('changeDate', function(event) {
				event.preventDefault();
				event.stopPropagation();
				var endTime = event.date;
				$('.validity-date').datetimepicker('setEndDate', endTime);
			});
		})(jQuery);
	</script>
